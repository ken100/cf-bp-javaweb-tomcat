#!/usr/bin/env ruby

# sync output
$stdout.sync = true
$stderr.sync = true

$:.unshift File.expand_path('../../lib', __FILE__)
require 'global'
require 'java_web_tomcat_pack'

puts "::::::::::::::::::::::::::::::::param1 : " + ARGV[0]
puts "::::::::::::::::::::::::::::::::param2 : " + ARGV[1]
puts "::::::::::::::::::::::::::::::::param3 : " + ARGV[2]

pack = JavaWebTomcatPack.new(Global.new(ARGV[0], ARGV[1]))
pack.compile
